{
    "llvm-target" : "i386-unknown-none-gnu",
    "target-endian" : "little",
    "target-pointer-width" : "32",
    "os" : "none",
    "target-env" : "gnu",
    "target_vendor" : "unknown",
    "arch" : "x86",
    "linker" : "gcc -m32",
    "pre-link-args" : "-Wl,--build-id=none -Wl,--gc-sections -static -ffreestanding -fno-builtin -nostdlib -nostartfiles -march=i386",
    "post-link-args" : "-lgcc",
    "cpu" : "i386",
    "dynamic-linking" : false,
    "executables" : false,
    "code-model" : "kernel",
    "no-compiler-rt" : true,
    "no-default-libraries" : true
}
